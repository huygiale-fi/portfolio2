{
    "family": "portfolio-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::380414078747:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "portfolio-container",
        "image": "placeholder",
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  